	<queryString>
		<![CDATA[declare 
 @fecha_inicial smalldatetime
,@fecha_final smalldatetime
--set @fecha_inicial=convert(smalldatetime,@fecha_i)
--set @fecha_final= convert(smalldatetime,@fecha_f)
set @fecha_inicial='01/09/2013 05:59:59'
set @fecha_final= '19/09/2013 23:59:59'


select tb_checador.folio_empleado
       ,tb_empleado.nombre+' '+tb_empleado.ap_paterno+' '+tb_empleado.ap_materno as nombre
       ,tb_establecimiento.nombre as establecimiento
       ,tb_checador.fecha_mov as fecha
       ,datename(dw, tb_checador.fecha_mov) as  dia
       ,CONVERT([varchar](8),tb_checador.fecha_mov,(108)) as  hora_registro 
       ,CONVERT([varchar](10),tb_checador.fecha_mov,(103)) as  fecha_corta  
       ,tb_checador.entosal as Movimiento
       ,tb_checador.pc_nombre
       ,tb_checador.pc_ip
       ,tb_checador.tipo_entrada
       ,tb_checador.status
       ,tb_checador.status_semana
       ,tb_checador.comida_receso
       ,tb_acumulado_inpuntualidad.dif_minutos
       ,tb_acumulado_inpuntualidad.minutos_a_favor
       ,tb_acumulado_inpuntualidad.status_semana 
       ,tb_acumulado_inpuntualidad.registros_faltan 
       ,CONVERT([varchar](20),@fecha_inicial,(113)) as fecha_inicial
       ,CONVERT([varchar](20),@fecha_final,(113)) as fecha_final
     
from tb_checador
  left outer join tb_acumulado_inpuntualidad on tb_acumulado_inpuntualidad.folio_empleado=tb_checador.folio_empleado and (CONVERT([varchar](5),tb_acumulado_inpuntualidad.fecha_mov,(108)))= (CONVERT([varchar](5),tb_checador.fecha_mov,(108)))
  inner join tb_empleado on tb_empleado.folio =tb_checador.folio_empleado 
  inner join tb_establecimiento on tb_empleado.establecimiento_id=tb_establecimiento.folio
where tb_checador.fecha_mov between @fecha_inicial and @fecha_final
GROUP BY tb_empleado.nombre+' '+tb_empleado.ap_paterno+' '+tb_empleado.ap_materno,CONVERT([varchar](8),tb_checador.fecha_mov,(108)),tb_checador.fecha_mov,tb_checador.folio_empleado,tb_establecimiento.nombre,tb_checador.entosal,tb_checador.pc_nombre,tb_checador.pc_ip,tb_checador.tipo_entrada,tb_checador.status,tb_checador.status_semana,tb_checador.comida_receso,tb_acumulado_inpuntualidad.dif_minutos,tb_acumulado_inpuntualidad.minutos_a_favor,tb_acumulado_inpuntualidad.status_semana,tb_acumulado_inpuntualidad.registros_faltan  
ORDER BY datename(dw, tb_checador.fecha_mov) desc,tb_establecimiento.nombre

]]>
	</queryString>
